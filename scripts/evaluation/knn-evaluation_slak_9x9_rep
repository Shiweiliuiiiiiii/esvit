#!/bin/bash
#SBATCH --job-name=knn_slak_tiny_imagenet_9x9_yaml
#SBATCH -p gpu
#SBATCH -N 1
#SBATCH --ntasks-per-node=1
#SBATCH --gpus-per-node=1
#SBATCH --gpus=1
#SBATCH -t 0-00:59:59
#SBATCH --exclusive
#SBATCH --cpus-per-task=18
#SBATCH -o knn_slak_tiny_imagenet_9x9_yaml.out

source /home/sliu/miniconda3/etc/profile.d/conda.sh
source activate LoRA



DATA_PATH=/projects/2/managed_datasets/imagenet/

OUT_PATH=~/project_space/esvit/output/esvit_exp/ssl/slak_tiny_imagenet_9x9_yaml
CKPT_PATH=~/project_space/esvit/output/esvit_exp/ssl/slak_tiny_imagenet_9x9_yaml/checkpoint.pth


# knn
python -m torch.distributed.launch --nproc_per_node=1 eval_knn.py --data_path $DATA_PATH \
--dump_features OUT_PATH/features/epoch100 \
--pretrained_weights $CKPT_PATH \
--checkpoint_key teacher --arch SLaK_tiny --kernel_size 9 9 9 9 5 --bn True \
--cfg experiments/imagenet/swin/slak_tiny_7x7_224.yaml \
--batch_size_per_gpu 256 MODEL.NUM_CLASSES 0

